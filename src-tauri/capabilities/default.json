{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "deep-link:default",
    "shell:allow-open",
    "dialog:allow-open",
    "opener:allow-reveal-item-in-dir",
    "log:default",
    "clipboard-manager:allow-read-text"
  ]
}
